<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.grg.face">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:name="com.lib.common.base.BaseApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".FaceActivity"
            android:theme="@style/Theme.AppCompat">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".single.SingleCameraActivity">
        </activity>
        <activity android:name=".living.SingleLivingCameraActivity">
        </activity>
        <activity android:name=".cameraview.FaceCheckCameraActivity">
        </activity>
        <activity android:name=".living.LivingCameraActivity">
        </activity>
        <activity android:name=".custom.CustomActivity">
        </activity>
        <activity android:name=".cameraview.TwoFaceCheckCameraActivity">
        </activity>
    </application>

</manifest>